{
    "name": "henrotaym/laravel-belgian-province-finder",
    "description": "Finding belgian province based on given criteria.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-package-versioning-config": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Henrotaym\\LaravelBelgianProvinceFinder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Henrotaym\\LaravelBelgianProvinceFinder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Henrotaym\\LaravelBelgianProvinceFinder\\Providers\\LaravelBelgianProvinceFinderServiceProvider"
            ]
        }
    }
}
